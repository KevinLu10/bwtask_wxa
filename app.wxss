.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
page{
  //background-color: #F8F8F8;
  background-color: #FFFFFF;
}

.load_more{
  border-radius: 10rpx;
  /* width: 20.31rem; */
  height: 75rpx;
  margin-top: 20rpx;
  font-size: 30rpx;
  color: #FFFFFF;
  text-align: center;
  margin-bottom: 50rpx;
  background-color: #FFFFFF;
  color: #666666;
  line-height: 75rpx;
  box-shadow: 0px 0px 5px 0px rgba(0,0,0,0.15);
}
